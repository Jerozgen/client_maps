{
	"schemaVersion": 1,
	"id": "client_maps",
	"version": "${version}",
	"name": "Client Maps",
	"description": "A mod to add client-side map storage to Minecraft, supporting Map in Slot and Minihud.",
	"authors": [
		"Kroojel"
	],
	"contact": {
		"homepage": "https://holebois.com",
		"sources": "https://github.com/holebois/client_maps",
		"issues": "https://github.com/holebois/client_maps/issues"
	},
	"license": "MIT",
	"icon": "assets/client_maps/icon.png",
	"environment": "Client",
	"entrypoints": {
		"client": [
			"com.holebois.client_maps.client_mapsClient"
		]
	},
	"mixins": [
		{
			"config": "client_maps.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.10",
		"minecraft": "~1.20.6",
		"java": ">=21",
		"fabric-api": "*"
	}
}
